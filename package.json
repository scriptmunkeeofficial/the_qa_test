{
  "name": "qa_test",
  "version": "1.0.0",
  "description": "simple web app to test the abilities of Quality Assurance interview candidates3",
  "license": "MIT",
  "author": "Ken Simeon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scriptmunkeeofficial/qa_test.git"
  },
  "homepage": "https://github.com/scriptmunkeeofficial/qa_test#readme",
  "bugs": {
    "url": "https://github.com/scriptmunkeeofficial/qa_test/issues"
  },
  "keywords": [
    "Interview",
    "QA",
    "QE",
    "Quality",
    "Quality Assurance",
    "Quality Engineer",
    "SDET"
  ],
  "config": {
    "loglevel": "verbose"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node ./src",
    "devstart": "nodemon --verbose ./src",
    "lint": "eslint src"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "ext": "js,json,ejs,scss",
    "delay": "2500"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bookshelf": "^1.2.0",
    "bookshelf-secure-password": "^5.0.0",
    "bootstrap": "^4.5.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "popper.js": "^1.16.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.0",
    "up": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "nodemon": "^2.0.4"
  }
}
