{
  "name": "qa_test",
  "version": "1.0.0",
  "description": "simple web app to test the abilities of Quality Assurance interview candidates3",
  "license": "MIT",
  "author": "Ken Simeon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scriptmunkeeofficial/qa_test.git"
  },
  "homepage": "https://github.com/scriptmunkeeofficial/qa_test#readme",
  "bugs": {
    "url": "https://github.com/scriptmunkeeofficial/qa_test/issues"
  },
  "keywords": [
    "QA",
    "QE",
    "Interview",
    "Quality",
    "Assurance",
    "Quality",
    "Engineer",
    "SDET"
  ],
  "config": {
    "loglevel": "verbose"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node ./src",
    "devstart": "nodemon --verbose ./src",
    "lint": "eslint src"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "ext": "js,json,ejs,scss",
    "delay": "2500"
  },
  "dependencies": {
    "async": "^3.1.0",
    "bookshelf": "^0.15.1",
    "bookshelf-schema": "^0.3.6",
    "bookshelf-secure-password": "^4.0.0",
    "bootstrap": "^4.3.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "knex": "^0.19.2",
    "mysql": "^2.17.1",
    "popper.js": "^1.15.0",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "nodemon": "^1.19.1"
  }
}
